{{#if books}}
    {{#each books}}
    <div class="container">
        <div class="card card-body">
        <h4>id: {{_id}}</h4>
        <h4>title : {{title}}</h4>
        <h4>author : {{author}}</h4>
        <h4>isbn: {{isbn}}</h4>
        <a href="/books/delete/{{_id}}" class="btn btn-danger">eliminar</a>
        </div>
    </div>
    {{/each}}
{{/if}}